{
    "name": "jundayw/laravel-firewall",
    "description": "A Laravel firewall whitelisting and blacklisting",
    "keywords": [
        "laravel",
        "firewall",
        "whitelist",
        "blacklist"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": ">=5.5",
        "geoip2/geoip2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Jundayw\\LaravelFirewall\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jundayw\\LaravelFirewall\\FirewallServiceProvider"
            ],
            "aliases": {
                "Firewall": "Jundayw\\LaravelFirewall\\Support\\Facades\\Firewall"
            }
        }
    }
}
